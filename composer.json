{
    "name": "appzap/migrator",
    "description": "A TYPO3 Extension for SQL Migrations",
    "authors": [
        {
            "name": "Sebastian Michaelsen",
            "email": "sebastian@app-zap.de",
            "role": "developer"
        },
        {
            "name": "portrino GmbH",
            "email": "dev@portrino.de",
            "homepage": "https://www.portrino.de",
            "role": "Developer"
        }
